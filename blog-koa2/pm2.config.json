{
  "apps": {
    "name": "blog-koa2",
    "script": "./bin/www",
    "watch": false,
    "ignore_watch": [
      "node_modules",
      "logs"
    ],
    "instances": 4,
    "error_file": "./logs/err.log",
    "out_file": "./logs/out.log",
    "log_date_format": "YYYY-MM-DD HH:mm Z",
    "watch_options": {
      "followSymlinks": false
    },
    "env": {
      "NODE_ENV": "development"
    },
    "env_production": {
      "NODE_ENV": "production"
    }
  }
}